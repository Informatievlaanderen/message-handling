{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "KafkaSettings": {
    "ClientConfig": {
      "bootstrap.servers": "localhost:9092",
      "client.id": "abc123",
      "max.in.flight": "1"
    },
    "Environment": "dev",
    "ProducerName": "municipality",
    "ProduceTopics": [
      {
        "Name": "events",
        "NumPartitions": 1,
        "ReplicationFactor": 1,
        "Configs": {
          "retention.bytes" : "-1",
          "retention.ms" : "-1"
        }
      },
      {
        "Name": "metrics",
        "NumPartitions": 1,
        "ReplicationFactor": 1,
        "Configs": {
          "cleanup.policy" : "delete",
          "retention.bytes" : "-1",
          "retention.ms" : "604800000"
        }
      }
    ]
  }
}
